<%- include("header") -%> 
    <div class="box" id="heading">
        <h1>It's a <%= listTitle %> !</h1>
    </div>
    <div class="box">

        <% newListItems.forEach(element=> { %>
            <div class="item">
                <input type="checkbox">
                <p>
                    <%= element %>
                </p>
            </div>
            <% }) %>

                <form action="/" method="POST" class="item">
                    <input type="text" name="newItem" placeholder="New Item" required autocomplete="off">
                    <button type="submit" name="list" value='<%= listTitle %>'>+</button>
                </form>
    </div>


<%- include("footer") -%> 